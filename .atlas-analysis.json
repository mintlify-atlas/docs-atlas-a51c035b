  "keyFeatures": [
    "Semantic vector search with OpenAI/Jina embeddings",
    "Typo-tolerant full-text/neural search with SPLADE",
    "Sub-sentence highlighting",
    "Recommendation engine",
    "RAG API with topic-based memory management",
    "Hybrid search with cross-encoder re-ranking",
    "Recency biasing and tunable merchandising",
    "Advanced filtering (date-range, tags, numeric)",
    "Group-based search for file-level results",
    "Analytics and CTR tracking",
    "Self-hosting support (Docker, Kubernetes)",
    "Multiple SDKs (TypeScript, Python, CLI)"
  ],
  "publicApiSurface": [
    "POST /api/chunk - Create chunks",
    "POST /api/chunk/search - Search chunks",
    "POST /api/chunk/autocomplete - Autocomplete",
    "POST /api/chunk_group - Create groups",
    "POST /api/chunk_group/search - Search groups",
    "POST /api/file - Upload files",
    "POST /api/message - Create chat messages",
    "POST /api/topic - Create chat topics",
    "POST /api/analytics - Get analytics",
    "PUT /api/analytics/ctr - Send CTR data",
    "POST /api/dataset - Create datasets",
    "POST /api/organization - Create organizations",
    "GET /api/events - Get events"
  ]
